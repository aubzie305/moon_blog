<div class = "container subtitle">Moon Feed
  <h4><%= link_to "Create New Article", '#' %> </h4>

  <ul>
    <% @articles.each do |article| %>
      <li>
        <%= link_to article.title, '#' %>
        <%= link_to "Edit", '#' %>
        <%= link_to "Delete", '#', method: :delete, 
            data: {confirm: "Are you sure you want to delete this article?"} %>
      </li>
    <% end %>
  </ul>

</div>


